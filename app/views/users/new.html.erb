<section class="signup">
  <h2>Sign Up</h2>
  <form action="<%= users_url %>" method="post">
    <%= render "shared/auth_token" %>
    <label class="email-label">Email Address
      <input class="email" type="text" name="user[email]">
    </label>
    <br>
    <label>Username
      <input class="username" type="text" name="user[username]">
    </label>
    <br>
    <label>Password
      <input class="password" type="password" name="user[password]">
    </label>
    <br>
    <button>Sign Up</button>
  </form>
</seciton>
